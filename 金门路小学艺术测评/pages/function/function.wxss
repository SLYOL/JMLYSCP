page {
  font-size: 28rpx;
  color: #6b6b6b;
  background: #f9f9f9;
}
.functionView {
  width: 94%;
  margin: 0 auto;
  margin-top: 20rpx;

}
.titleView {
  line-height: 100rpx;
}
.card {

  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;

}
.cardChild {
  margin-top: 20rpx;
  width: 47%;
  border-radius: 15rpx;
  background: #ababab;
  box-shadow: 0 0rpx 10rpx #ababab;
}
.cardChildImage {
  width: 100%;
  height: 190rpx;
}
.cardChildImage2{
  width: 100%;
  height: 150rpx;
}
.cardChild2 {
  margin-top: 20rpx;
  width: 100%;
  border-radius: 15rpx;
  background: #ababab;
  box-shadow: 0 2rpx 10rpx #ababab;
}
.export {
  position: fixed;
  top: 10px;
  right: 10%;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background-color: #0059BA;
  border-radius: 40rpx;
  color: white;


}